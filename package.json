  "description": "An HTML5 Canvas / WebGL monster hunting game",
  "main": "src/server/index.ts",
  "scripts": {
    "start": "npm run server:start",
    "server:start": "nodemon --watch src --eval 'ts-node -r dotenv/config' src/server/index.ts",
    "web:start": "webpack-dev-server --config webpack.config.js"
