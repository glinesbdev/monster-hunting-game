    "TypeScript",
    "Game",
    "HTML5",
    "Canvas",
    "WebGL",
    "Monter",
    "Hunting"
  ],
  "author": "Bradyn Glines",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/glinesbdev/monster-hunting-game/issues"
  },
  "homepage": "https://github.com/glinesbdev/monster-hunting-game#readme",
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "nodemon": "^1.19.1",
    "phaser": "^3.17.0",
    "ts-node": "^8.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.3",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
